{
  "_version": "1.76.0",
  "sap.app": {
    "id": "apm.orders",
    "type": "application",
    "title": "Orders",
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/festival/",
        "type": "OData",
        "settings": { "odataVersion": "4.0" }
      }
    }
  },
  "sap.ui5": {
    "preload": "off",
    "dependencies": {
      "minUI5Version": "1.143.2",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.ui.layout": {}
      }
    },
    "models": {
      "": {
        "dataSource": "mainService",
        "type": "sap.ui.model.odata.v4.ODataModel",
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.f.routing.Router",
        "viewType": "XML",
        "viewPath": "apm.orders.view",
        "controlId": "ordersFcl",
        "controlAggregation": "beginColumnPages",
        "transition": "slide",
        "async": true
      },
      "routes": [
        { "name": "OrderList", "pattern": "", "target": ["OrderList"] },
        { "name": "OrderDetail", "pattern": "orders/{ID}", "target": ["OrderList", "OrderDetail"] }
      ],
      "targets": {
        "OrderList": {
          "viewName": "OrderList",
          "controlAggregation": "beginColumnPages"
        },
        "OrderDetail": {
          "viewName": "OrderDetail",
          "controlAggregation": "midColumnPages"
        }
      },
      "bypassed": {
        "target": "OrderList"
      }
    },
    "rootView": {
      "viewName": "apm.orders.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
